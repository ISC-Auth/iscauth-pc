<Window x:Class="WifiCollection.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WifiCollection"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="output" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="271" Width="497" ScrollViewer.VerticalScrollBarVisibility="Auto"></TextBox>
        <TextBox x:Name="url_textbox" HorizontalAlignment="Left" Height="23" Margin="10,286,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="262"/>
        <Button x:Name="connect_button" Content="Connect" HorizontalAlignment="Left" Margin="311,286,0,0" VerticalAlignment="Top" Width="75" Click="connect_button_Click"/>

    </Grid>
</Window>
